
RewriteEngine On

RewriteRule ^([a-zA-Z0-9_-]+)(|/)$ index.php?url=$1
RewriteRule ^news/edit/([a-zA-Z0-9_-]+)(|/)$ index.php?url=editnews&news=$1
RewriteRule ^news/delete/([a-zA-Z0-9_-]+)(|/)$ index.php?url=news&delete=$1
RewriteRule ^bans/delete/([a-zA-Z0-9_-]+)(|/)$ index.php?url=bans&delete=$1
RewriteRule ^report/delete/([a-zA-Z0-9_-]+)(|/)$ index.php?url=report&delete=$1
RewriteRule ^badges/delete/([a-zA-Z0-9_-]+)(|/)$ index.php?url=badges&delete=$1
RewriteRule ^gebruiker/([a-zA-Z0-9_-]+)(|/)$ index.php?url=gebruiker&user=$1
RewriteRule ^gebrank/([a-zA-Z0-9_-]+)(|/)$ index.php?url=gebrank&user=$1
RewriteRule ^giveuseroftheweek/([a-zA-Z0-9_-]+)(|/)$ index.php?url=giveuseroftheweek&user=$1